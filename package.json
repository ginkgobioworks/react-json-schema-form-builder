{
  "name": "react-json-schema-form-builder",
  "version": "1.0.0",
  "description": "React JSON Schema Form Editor",
  "author": "nathanalam",
  "license": "Apache 2.0",
  "repository": "ginkgobioworks/react-json-schema-form-builder",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:flow test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:flow": "flow",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom --coverage --coverageDirectory=coverage/",
    "test:watch": "react-scripts test --env=jsdom",
    "predeployDemo": "cd example && npm install && npm run build",
    "deployDemo": "gh-pages -d example/build",
    "prepublishOnly": "npm test && echo 'PASSED ALL TESTS'"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@rjsf/core": "^2.3.0",
    "babel-eslint": "^10.0.3",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.139.0",
    "flow-typed": "^3.2.1",
    "gh-pages": "^2.2.0",
    "jsoneditor": "^9.1.4",
    "jsoneditor-react": "^3.1.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-json-editor-ajrm": "^2.5.13",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "js-yaml": "^3.14.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-bootstrap": "^1.3.0",
    "react-jss": "^10.4.0",
    "react-select": "^3.1.0",
    "react-spinners": "^0.9.0",
    "react-switch": "^5.0.1",
    "reactstrap": "^8.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  }
}
