{
  "name": "@ginkgo-bioworks/react-json-schema-form-builder",
  "version": "1.0.6",
  "description": "React Component for visually configuring a JSON Schema-based form",
  "author": {
    "name": "Nathan Alam",
    "email": "nathanalam99@gmail.com",
    "url": "https://github.com/nathanalam"
  },
  "license": "Apache-2.0",
  "repository": "ginkgobioworks/react-json-schema-form-builder",
  "keywords": [
    "react",
    "form",
    "json-schema",
    "visual"
  ],
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "clean": "rm package-lock.json && rm -r -f node_modules && rm -r -f dist && rm -r -f coverage && cd example && rm package-lock.json && rm -r -f node_modules && rm -r -f build",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "flow": "flow",
    "flow:install-types": "flow-typed install",
    "test": "run-s test:unit test:lint test:flow",
    "test:flow": "flow && cd example && npm install && flow",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom --coverage --coverageDirectory=coverage/",
    "test:watch": "react-scripts test --env=jsdom",
    "predeployDemo": "cd example && npm install && npm run build",
    "deployDemo": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.x || ^17.x"
  },
  "files": [
    "/dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@rjsf/core": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "flow-bin": "^0.92.1",
    "flow-typed": "^3.2.1",
    "gh-pages": "^2.2.0",
    "jsoneditor": "^9.1.4",
    "jsoneditor-react": "^3.1.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-json-editor-ajrm": "^2.5.13",
    "react-scripts": "^3.4.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-beautiful-dnd": "^13.0.0",
    "react-bootstrap": "^1.3.0",
    "react-jss": "^10.4.0",
    "react-select": "^3.1.0",
    "react-spinners": "^0.9.0",
    "react-switch": "^5.0.1",
    "reactstrap": "^8.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/ginkgobioworks/react-json-schema-form-builder/issues",
    "email": "devs@ginkgobioworks.com"
  }
}
