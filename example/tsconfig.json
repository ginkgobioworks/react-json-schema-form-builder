{
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react",
    "module": "CommonJS",
    "declaration": true,
    "declarationMap": true,
    "outDir": "../dist/example",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "reactstrap": ["node_modules/reactstrap/dist/reactstrap.modern.js"]
    }
  },
  "include": ["./src/**.ts"]
}
